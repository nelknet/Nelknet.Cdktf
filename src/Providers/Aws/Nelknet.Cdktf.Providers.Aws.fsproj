<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <RootNamespace>Nelknet.Cdktf.Providers.Aws</RootNamespace>
    <AssemblyName>Nelknet.Cdktf.Providers.Aws</AssemblyName>
    <PackageId>Nelknet.Cdktf.Providers.Aws</PackageId>
    <Authors>Nelknet</Authors>
    <Company>Nelknet</Company>
    <Description>F# computation expression for the CDKTF provider hashicorp/aws (version 5.100.0).</Description>
    <PackageReleaseNotes>Generated from hashicorp/aws@=5.100.0.</PackageReleaseNotes>
    <PackageTags>cdktf;terraform;fsharp;aws</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/nelknet/Nelknet.Cdktf</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/nelknet/Nelknet.Cdktf</PackageProjectUrl>
  </PropertyGroup>
  <PropertyGroup>
    <ProviderId>aws</ProviderId>
    <ProviderModule>Aws</ProviderModule>
    <ProviderNamespace>Nelknet.Cdktf.Providers.Aws</ProviderNamespace>
    <ProviderSource>hashicorp/aws</ProviderSource>
    <ProviderVersion>5.100.0</ProviderVersion>
    <ProviderPackageDir>$(RepoRoot)generated/aws</ProviderPackageDir>
    <GeneratedOutputRoot>$(MSBuildProjectDirectory)\Generated</GeneratedOutputRoot>
    <ProviderAssemblyPath>$(ProviderPackageDir)\bin\$(Configuration)\net8.0\aws.dll</ProviderAssemblyPath>
  </PropertyGroup>
  <Import Project="$(RepoRoot)build/CodeGen.targets" />
  <ItemGroup>
    <ProjectReference Include="../../Core/Nelknet.Cdktf.Core/Nelknet.Cdktf.Core.fsproj" />
    <!-- Reference the generated provider assembly directly so we can bundle it into the NuGet package -->
    <Reference Include="$(ProviderId)"
               Condition="Exists('$(ProviderAssemblyPath)')">
      <HintPath>$(ProviderAssemblyPath)</HintPath>
      <Private>true</Private>
    </Reference>
    <!-- Ensure CodeGen project is built before running generation -->
    <ProjectReference Include="$(RepoRoot)tools/Nelknet.Cdktf.CodeGen/Nelknet.Cdktf.CodeGen.fsproj"
                      ReferenceOutputAssembly="false"
                      PrivateAssets="all"
                      Pack="false" />
  </ItemGroup>
  <ItemGroup>
    <None Include="../../../README.md" Pack="true" PackagePath="" Link="README.md" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="$(ProviderAssemblyPath)"
             Pack="true"
             PackagePath="lib/net8.0/"
             Condition="Exists('$(ProviderAssemblyPath)')" />
    <Content Include="$(ProviderPackageDir)\bin\$(Configuration)\net8.0\$(ProviderId).pdb"
             Pack="true"
             PackagePath="lib/net8.0/"
             Condition="Exists('$(ProviderPackageDir)\bin\$(Configuration)\net8.0\$(ProviderId).pdb')" />
  </ItemGroup>
  <!-- Include generated F# files (CodeGen.targets ensures they exist before build) -->
  <ItemGroup>
    <Compile Include="Generated/Resources/**/*.fs" />
    <Compile Include="Generated/$(ProviderModule).Module.fs" />
  </ItemGroup>
  <Target Name="RemoveBundledBindingDependency" AfterTargets="GetPackageDependencies">
    <ItemGroup>
      <PackageDependencies Remove="@(PackageDependencies)"
                           Condition="'%(PackageDependencies.Identity)' == '$(ProviderId)'" />
    </ItemGroup>
  </Target>
</Project>
